<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <EnforceCodeStyleInBuild>True</EnforceCodeStyleInBuild>
    <AssemblyName>RLBot</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\RLBotModels\RLBotModels.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="FlatBuffers">
      <HintPath>lib\FlatBuffers.dll</HintPath>
    </Reference>
    <Reference Include="RLBotSecret">
      <HintPath>RLBotSecret.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
